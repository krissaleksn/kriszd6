<Window x:Class="kristinazd5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" Background="AliceBlue"
        xmlns:local="clr-namespace:kristinazd5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>

            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Content="Задание 1" Click="Button_Click" FontSize="16"></Button>
            <Button Content="Задание 2" Click="Button_Click_1" FontSize="16" ></Button>
            <Button Content="Задание 3" Click="Button_Click_2" FontSize="16"></Button>
            <Button Content="Задание 4" Click="Button_Click_3" FontSize="16"></Button>
            <Button Content="Задание 5" Click="Button_Click_4" FontSize="16"></Button>
            <Button Content="Задание 6" Click="Button_Click_5" FontSize="16"></Button>
        </StackPanel>
        <Frame Grid.Row="1" NavigationUIVisibility="Hidden" Name="myframe"></Frame>
    </Grid>
</Window>
